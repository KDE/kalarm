#: recurrenceedit.cpp:334
#, kde-format
msgctxt "@option:check"
msgid "Only during working time"
msgstr "Tikai darba laikā"

#: recurrenceedit.cpp:337
#, kde-kuit-format
msgctxt "@info:whatsthis"
msgid ""
"<para>Only execute the alarm during working hours, on working days.</"
"para><para>You can specify working days and hours in the Configuration "
"dialog.</para>"
msgstr ""
"<para>Izpildīt signālus tikai darba laikā darba dienās.</para><para>Varat "
"norādīt darba dienas un laiku konfigurācijas lodziņā.</para>"

