msgctxt "@option:radio"
msgid "End after:"
msgstr "Sonra bitir:"

#: recurrenceedit.cpp:212
#, kde-format
msgctxt "@info:whatsthis"
msgid "Repeat the alarm for the number of times specified"
msgstr "Alarmı belirtilen bir sayı kadar yinele"

#: recurrenceedit.cpp:218
#, kde-format
