msgstr "Selecteer de vakantieregio die u wilt gebruiken."

#. i18n: ectx: label, entry (DefaultFgColour), group (General)
#: data/kalarmconfig.kcfg:93 kalarmcalendar/kaeventformatter.cpp:92
#, kde-format
msgctxt "@label"
msgid "Foreground color"
msgstr "Voorgrondkleur"

#. i18n: ectx: whatsthis, entry (DefaultFgColour), group (General)
#: data/kalarmconfig.kcfg:94
#, kde-format
msgctxt "@info:whatsthis"
msgid "Default foreground color for alarm message windows."
msgstr "Standaard voorgrondkleur voor alarmvensters"

#. i18n: ectx: label, entry (DefaultBgColour), group (General)
#: data/kalarmconfig.kcfg:98 kalarmcalendar/kaeventformatter.cpp:94
#, kde-format
