<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.kalarm.notify">
    <method name="alarmDaemonUpdate">
      <arg name="calendarStatus" type="i" direction="in"/>
    </method>
    <method name="handleEvent">
      <arg name="eventID" type="s" direction="in"/>
    </method>
    <method name="registered">
      <arg name="reregister" type="b" direction="in"/>
      <arg name="result" type="i" direction="in"/>
    </method>
    <method name="cacheDownloaded">
      <arg name="resourceID" type="s" direction="in"/>
    </method>
  </interface>
</node>
